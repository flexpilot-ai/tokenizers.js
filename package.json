{
  "name": "@flexpilot-ai/tokenizers.js",
  "version": "2.17.2",
  "description": "JavaScript implementation of Hugging Face's tokenizers based on transformers.js",
  "main": "./src/tokenizers.js",
  "types": "./types/tokenizer.d.ts",
  "type": "module",
  "scripts": {
    "typegen": "tsc ./src/tokenizers.js --allowJs --declaration --emitDeclarationOnly --declarationMap --outDir types",
    "dev": "webpack serve --no-client-overlay",
    "build": "webpack && npm run typegen"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/flexpilot-ai/tokenizers.js"
  },
  "keywords": [
    "transformers",
    "transformers.js",
    "huggingface",
    "hugging face",
    "machine learning",
    "deep learning",
    "artificial intelligence",
    "AI",
    "ML",
    "tokenizer",
    "huggingface tokenizer"
  ],
  "author": "mohankumarelec",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/flexpilot-ai/tokenizers.js/issues"
  },
  "homepage": "https://github.com/flexpilot-ai/tokenizers.js#readme",
  "dependencies": {
    "@huggingface/jinja": "^0.2.2"
  },
  "devDependencies": {
    "jsdoc-to-markdown": "^8.0.1",
    "typescript": "^5.2.2",
    "webpack": "^5.80.0",
    "webpack-cli": "^5.0.2",
    "webpack-dev-server": "^4.13.3"
  },
  "files": [
    "src",
    "dist",
    "types",
    "README.md",
    "LICENSE"
  ],
  "browser": {
    "fs": false,
    "path": false,
    "url": false
  },
  "publishConfig": {
    "access": "public"
  },
  "jsdelivr": "./dist/tokenizer.min.js",
  "unpkg": "./dist/tokenizer.min.js"
}
